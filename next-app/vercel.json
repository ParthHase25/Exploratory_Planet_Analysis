{
    "rewrites": [
        {
            "source": "/api/auth/:path*",
            "destination": "/api/auth/:path*"
        },
        {
            "source": "/api/data/:path*",
            "destination": "/api/data/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}